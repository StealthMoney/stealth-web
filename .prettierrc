{
	"$schema": "https://json.schemastore.org/prettierrc",
	"endOfLine": "lf",
	"printWidth": 80,
	"singleQuote": false,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "es5",
	"semi": false,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "always",
	"plugins": ["prettier-plugin-tailwindcss"]
}
